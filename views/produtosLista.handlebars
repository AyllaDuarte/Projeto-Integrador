<main class="cadastro">
  <header class="cadastro__titulo">
    <h1 class="cadastro-main__title"> Lista de produtos </h1>
  </header>
  <div class="cadastro__form">
    <section>
      <div class="listaDePedidos">
            <table>
               <tr> 
                <th>Id </th>
                <th>Nome </th>
                <th>Quantidade</th>
                <th>Preço</th>
                <th>Descrição</th>
                <th>Editar</th>
                <th>Excluir</th>
                </tr>
              {{#each productList}}
                <tr>
               <td>{{id_prod}}</td>
               <td>{{name_prod}}</td>
               <td>{{stock_prod}}</td>
               <td>R${{price_prod}}</td>
               <td>{{description_prod}}</td>
               <td><a href="/editar-produto/{{id_prod}}"><button><i><span class="material-symbols-outlined">
               edit
               </span></i></button></a></td>
               <td><a href="/del-produto/{{id_prod}}"><button><i><span class="material-symbols-outlined">
                delete
               </span></i></button></a></td>
                </tr>
              {{/each}}
            </table>
      </div>
    </section>
  </div>
    <div class="cadastro--btn-back">
      <a href="/cadastroProduto"> Voltar </a>
    </div>
  </div>
</main>